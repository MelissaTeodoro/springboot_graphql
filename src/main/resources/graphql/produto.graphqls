type Produto {
    id: ID!,
    nome: String,
    valor: Int!
}

input ProdutoInput {
    id: ID,
    nome: String,
    valor: Int
}

extend type Query {
    produto(id: ID!): Produto,
    produtos: [Produto]!
}

extend type Mutation {
    saveProduto(produto: ProdutoInput!): Produto
    deleteProduto(id: ID!): Boolean
}